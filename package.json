{
  "name": "angular-webpack-starter",
  "version": "1.0.0",
  "description": "Starter for Angular project with webpack",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "start-first-example-app": "webpack serve --config config/webpack/first-example-app/fea-webpack.dev.js --inline --progress --port 3000 --hot",
    "start-second-example-app": "webpack serve --config config/webpack/second-example-app/sea-webpack.dev.js --inline --progress --port 3000 --hot",
    "build-first-example-app": "rimraf dist/dist-fea && webpack --config config/webpack/first-example-app/fea-webpack.prod.js --progress --profile --bail",
    "build-second-example-app": "rimraf dist/dist-sea && webpack --config config/webpack/second-example-app/sea-webpack.prod.js --progress --profile --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DigitalCrafting/angular-webpack-starter.git"
  },
  "keywords": [
    "angular",
    "webpack",
    "karma",
    "jasmine",
    "starter"
  ],
  "author": "Kamil Kasprzyk",
  "license": "Apache 2.0",
  "bugs": {
